{
  "acpScripts": [ "admin.js" ],
  "description": "A widget that shows your rss feeds",
  "hooks": [
    { "hook": "filter:admin.header.build", "method": "filterAdminHeaderBuild" },
    { "hook": "filter:widget.render:rss", "method": "filterWidgetRenderRss" },
    { "hook": "filter:widgets.getWidgets", "method": "filterWidgetsGetWidgets" },
    { "hook": "static:app.load", "method": "staticAppLoad" }
  ],
  "id": "nodebb-widget-rss",
  "library": "plugin.js",
  "name": "NodeBB RSS",
  "nbbpm": {
    "compatibility": "^1.18.0"
  },
  "templates": "templates",
  "url": "https://github.com/Jenkler/nodebb-widget-rss"
}
